      <html_code><![CDATA[<script>
  function get_db()
  {
    var InnovatorServerRoot = document.getElementById("input_url").value;
	if(InnovatorServerRoot)
	{
		var qry1=top.aras.newIOMItem("Method");
		qry1.setProperty("url",InnovatorServerRoot);
		qry1=qry1.apply("server_get_dblist_for_url");
                                           var res=qry1.newXMLDocument();
                                           res.loadXML(qry1.getResult());

	  var dbs = res.selectNodes("/DBList/DB"); 
	  var database_select_elem = document.getElementById("select_db");	  
	  for (var i=0; i < dbs.length; i++) {
		var db = dbs[i];
		var elOptNew = document.createElement('option');
		elOptNew.text = db.getAttribute("id");
		database_select_elem.add(elOptNew);
	  } 
	}
  }
  
  
  function return_values()
  {
;
    if(document.getElementById("input_url").value)
	{
     var res="";
res+="<Item>";	
 res+="<url>"+document.getElementById("input_url").value+"</url>";
	 var db=document.getElementById("select_db");
	 res+="<db>"+db.options[db.selectedIndex].value+"</db>";
	 res+="<user>"+document.getElementById("input_user").value+"</user>";
	 res+="<pw>"+document.getElementById("input_pw").value+"</pw>";
                      res+="</Item>";
	}
       //alert(res);
	window.returnValue=res;
	window.close();

  }

</script>
<table>
<tr>
